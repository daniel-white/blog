{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.11",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^3.1.1",
    "@lekoarts/gatsby-theme-minimal-blog-core": "^3.1.1",
    "axios": "^0.21.4",
    "axios-jsonp": "^1.0.4",
    "gatsby": "^3.14.0",
    "gatsby-omni-font-loader": "^1.3.1",
    "gatsby-plugin-feed": "^3.14.0",
    "gatsby-plugin-gatsby-cloud": "^3.1.0",
    "gatsby-plugin-google-analytics": "^3.13.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-offline": "^4.13.0",
    "gatsby-plugin-postcss": "^4.13.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.25",
    "gatsby-transformer-remark": "^4.10.0",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md,jsx,ts,tsx,yaml,mdx,json}": "prettier --write"
  }
}
